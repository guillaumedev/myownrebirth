<ul>
    {% for twitch in listTwitch %}
        <div class="col-md-3 col-sm-6">
            <div class="panel panel-default text-center">
                <div class="panel-heading">
                    <h3>{{ twitch.seed }}</h3>
                </div>
                <div class="panel-body">
                    <p>{{ twitch.firstPlayer }} vs {{twitch.secondPlayer }}</p>
                    <a href="{{ path('gdev_viewTwitch',{'id': twitch.id}) }}" class="btn btn-primary">Voir</a>
                </div>
            </div>
        </div>
    {% endfor %}
</ul>